export TERM="xterm-256color"

alias logo="git log --pretty=oneline"
alias ytune="youtube-dl -x --audio-quality 0 --audio-format mp3"
alias youtune="~/Documents/code/bash/youtune/youtune.sh"
alias fopen="open finder ."
alias open="open -a "
alias apache_start="sudo apachectl start"
alias apache_stop="sudo apachectl stop"
alias apache_restart="sudo apachectl restart"
alias hide="chflags hidden"
alias unhide="chflags nohidden"
export CLICOLOR=1
if which rbenv > /dev/null; then eval "$(rbenv init -)"; fi

#stuff useful only for current user
source ".bash_profile_user"
